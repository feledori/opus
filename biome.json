{
	"$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"nursery": {
				"useImportType": "warn",
				"useExportType": "warn",
				"noUnusedImports": "warn"
			}
		}
	},
	"files": {
		"ignore": [".expo/", "expo-env.d.ts"]
	}
}
